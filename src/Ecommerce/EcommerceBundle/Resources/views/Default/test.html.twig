{{ dump(produits)  }}

{% for produit in produits %}
    <section>
        <u1>
            <li>{{ produit.nom }}</li>
            <li>{{ produit.categorie }}</li>
            <li>{{ produit.description }}</li>
            <li>{{ produit.disponible }}</li>
            <li>{{ produit.image }}</li>
            <li>{{ produit.prix }}</li>
            <li>{{ produit.tva }}</li>
        </u1>
    </section>
{% endfor %}